PORT=8080
HOST=0.0.0.0
DATABASE_URL=postgres://postgres:postgres@postgres:5432/agent_identity
REDIS_URL=redis://redis:6379
ISSUER_KID=identity-ca-v1
ISSUER_PRIVATE_JWK_FILE=/run/secrets/issuer_private_jwk.json
# KMS_KEY_ARN=arn:aws:kms:us-east-1:123456789012:key/xxx
# AWS_REGION=us-east-1
TIMESTAMP_SKEW_MS=60000
NONCE_TTL_SECONDS=120
# SECURITY: Replace with a strong key (min 16 chars). Never commit .env.
ADMIN_API_KEY=replace-with-strong-admin-api-key
# Exchange API key for JWT: POST /v1/admin/token { "api_key": "..." }
ADMIN_JWT_TTL_SECONDS=900
RATE_LIMIT_PER_MINUTE=120
NODE_ENV=production
