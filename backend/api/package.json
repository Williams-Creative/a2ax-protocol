{
  "name": "a2ax-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prebuild": "npm run build --prefix ../../protocol",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest run --pool=threads --exclude tests/integration/**",
    "test:watch": "vitest --pool=threads",
    "test:unit": "vitest run --pool=threads --exclude 'tests/integration/**'",
    "test:integration": "vitest run --pool=threads tests/integration",
    "lint": "eslint .",
    "keygen:issuer": "node scripts/generate-issuer-jwk.mjs",
    "pilot:smoke": "node scripts/pilot-smoke.mjs"
  },
  "dependencies": {
    "@a2ax/protocol": "file:../../protocol",
    "@aws-sdk/client-kms": "^3.700.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/sensible": "^6.0.3",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "ioredis": "^5.4.1",
    "jose": "^5.9.6",
    "pg": "^8.13.1",
    "pino": "^9.5.0",
    "prom-client": "^15.1.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
